  if (item.quantity === 0) {\n                this.$emit('removeFromCart', item)\n            }\n\n            this.updateCart()\n        },\n        incrementQuantity(item) {\n            item.quantity += 1\n\n            this.updateCart()\n        },\n        updateCart() {\n            localStorage.setItem('cart', JSON.stringify(this.$store.state.cart))\n        },\n        removeFromCart(item) {\n            this.$emit('removeFromCart', item)\n\n            this.updateCart()\n        },\n    },\n}\n</script>"]}]}